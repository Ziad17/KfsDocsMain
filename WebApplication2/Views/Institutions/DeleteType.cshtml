@model WebApplication2.Models.ViewModels.Institutions.DeleteInstitutionTypeModel

@{ ViewBag.Title = "مسح نوع مؤسسة";}
<h3>هل أنت متأكد من المسح؟</h3>
<div>
    <h2>نوع المؤسسة</h2>
    <hr />
    <div class="data_labels">
        @Html.LabelFor(m => m.Name)
        <h5>
            @Model.Name
        </h5>

        @Html.LabelFor(m => m.Desc)
        <h5>
            @Model.Desc
        </h5>


    </div>

    @using (Html.BeginForm()) {
        @Html.AntiForgeryToken()

        <div class="form-actions no-color">
            <input type="submit" value="مسح" class="btn btn-default" /> 
        </div>
    }
</div>
